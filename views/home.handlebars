<link href="https://fonts.googleapis.com/css?family=Permanent+Marker" rel="stylesheet">
<script>
    function openAuth(event){
      event.preventDefault()

      const url = '/authorize'
      const title = 'lyrebirdAuth'
      const w = 550
      const h = 700

      var top = window.top.outerHeight / 2 + window.top.screenY - (h / 2)
      var left = window.top.outerWidth / 2 + window.top.screenX - (w / 2)

      var newWindow = window.open(url, title, 'toolbar=no, location=no, directories=no, status=no, menubar=no, resizable=no, copyhistory=no, scrollbars=yes, width=' + w + ', height=' + h + ', top=' + top + ', left=' + left);

      window.addEventListener('message', event => {
        newWindow.close()
        if (event.origin !== '{{publicUrl}}') {
          return
        }
        location.reload()
      }, false)

      if (window.focus) {
        newWindow.focus()
      }
    }
    function vote(note){
        window.location = '/vote?note='+note
    }
</script>
<style>
    .title {
        font-size: 20px;
        font-family: 'Permanent Marker', cursive;
        position: absolute;
        left: 60px;
        top: 30px;
    }
    .title-icon {
        font-size: 14px;
        position: absolute;
        top: 10px;
    }
</style>

<div class="top-menu">
  <img class="title-icon" width="60" height="60" src="data:image/svg+xml;utf8;base64," />

  <p class="title">LVT</p>
</div>

<h1>Let's rate it</h1>
<h2>Discover the best synthetic voice in town. Vote for the most realistic, most beautiful voice ever created!</h2>

{{#if audio}}
<audio style="display:none;" id="player" controls>
  <source src="{{audio}}" type="audio/wav">
</audio>
{{/if}}

{{#if participants}}
<div style="margin:0" class="container">
  <a onclick="document.getElementById('player').play()" href="#">Hear {{name}}'s voice</a>
</div>

<svg class="icon-source">
    <defs>
        <path id="star" d="M12,1.776l3.286,6.779l7.464,1.032l-5.433,5.221l1.326,7.417L12,18.67l-6.645,3.555l1.327-7.417L1.25,9.587l7.464-1.032L12,1.776z"/>
    </defs>
</svg>

<p dir="rtl">
    <button onclick="vote(5)" title="wow" class="wow-good-rate">
        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1990/link" viewBox="0 0 24 24">
            <use xlink:href="#star" class="icon-star" />
        </svg>
    </button>
    <button onclick="vote(4)"  title="awesome" class="very-good-rate">
        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1990/link" viewBox="0 0 24 24">
            <use xlink:href="#star" class="icon-star" />
        </svg>
    </button>
    <button onclick="vote(3)"  title="pretty good" class="good-rate">
        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1990/link" viewBox="0 0 24 24">
            <use xlink:href="#star" class="icon-star" />
        </svg>
    </button>
    <button onclick="vote(2)"  title="cool" class="normal-rate">
        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1990/link" viewBox="0 0 24 24">
            <use xlink:href="#star" class="icon-star" />
        </svg>
    </button>
    <button onclick="vote(1)"  title="so-so" class="bad-rate">
        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1990/link" viewBox="0 0 24 24">
            <use xlink:href="#star" class="icon-star" />
        </svg>
    </button>
</p>
{{/if}}

<div class='container'>
    <a href='/authorize' onClick='openAuth(event)'>Participate with your voice avatar!</a>
</div>

<h1>Leaderboard</h1>
<table>
    <tr>
        <th>
            Position
        </th>
        <th>
            Voice
        </th>
        <th>
            Name
        </th>

        <th>
            Rate
        </th>
    </tr>
    {{#each participants}}
    <tr>
        <td>
            #{{math @index "+" 1}}

        </td>
        <td>
            <audio style="display:none" id="{{@index}}" controls>
                <source src="{{audio}}" type="audio/wav">
            </audio>
            <div  style="margin:0; text-align:left" class="container">
                <a  style="padding-bottom:10px;  padding-top:10px;" onclick="document.getElementById('{{@index}}').play()" href="#">Play</a>
            </div>
        </td>
        <td>
            {{name}}
        </td>
        <td>
            {{note}}
        </td>
    </tr>
    {{/each}}
</table>
